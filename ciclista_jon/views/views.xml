            <field name="type"></field>
            <field name="res_model"></field>
            <field name="view_mode"></field>
            <field name="view_type">form</field>
            <menuitem name="UniversidadJon" id="ciclista_jon_menu_principal"/>
            <menuitem name="Ciclista" id="ciclista_jon_submenu" paren="ciclista_jon_menu_principal"/>
        </record>

