        <!-- explicit list view definition -->
<!-- ======================================================================= -->
        <record model="ir.ui.view" id="ciclista_ciclista_jon_tree_view">
            <field name="name">ciclista_ciclista_jon_tree_view</field>
            <field name="model">ciclista.ciclista_jon</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="codigo" />
                    <field name="nombre" />
                    <field name="descripcion" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="ciclista_ciclista_jon_tree_view">
            <field name="name">ciclista_ciclista_jon_tree_view</field>
            <field name="model">ciclista.ciclista_jon</field>
            <field name="arch" type="xml">
                <form string="Gestión de una ciclista">
                    <sheet>
                        <group string="Géstion de una ciclista">
                            <field name="codigo"/>
                            <field name="nombre"/>
                            <field name="descripcion"/>
                            <field name="pertenece"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="ciclista_ciclista_jon_tree_view">
