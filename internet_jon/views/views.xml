        <record model="ir.ui.view" id="producto_internet_jon_tree_view">
            <field name="name">producto_uni_jon_tree_view</field>
            <field name="model">producto.internet_jon</field>
            <field name="arch" type="xml"></field>
                <tree>
                    <field name="codigo" />
